{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "contact_add_to_list",
  "type": "object",
  "additionalProperties": false,
  "properties": {
    "account_id": {
      "type": "string",
      "description": "Microsoft account ID used to perform the copy."
    },
    "contact_id": {
      "type": "string",
      "description": "Identifier of the contact to add to the list."
    },
    "list_id": {
      "type": "string",
      "description": "Target contact list/folder identifier."
    }
  },
  "required": ["account_id", "contact_id", "list_id"]
}
